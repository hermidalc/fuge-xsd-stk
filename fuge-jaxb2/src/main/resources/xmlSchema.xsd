      <xsd:documentation> The upper limit of a range value. </xsd:documentation>
	 </xsd:annotation>
    </xsd:attribute>	 


    
    </xsd:extension>
    </xsd:complexContent>

  </xsd:complexType>

  
  <xsd:complexType name="FuGE.Common.Measurement.MeasurementType" abstract = "true">
