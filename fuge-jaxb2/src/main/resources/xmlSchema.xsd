          <xsd:documentation> A value given by an ontology term. </xsd:documentation>
		 </xsd:annotation>
               <xsd:complexType>
        			<xsd:attribute name="OntologyTerm_ref" use="required" type="xsd:string"/>
        	   </xsd:complexType>
        </xsd:element>
       

    </xsd:sequence> 



    
    </xsd:extension>
    </xsd:complexContent>

  </xsd:complexType>

  
  <xsd:complexType name="FuGE.Common.Measurement.RangeType" abstract = "false">
