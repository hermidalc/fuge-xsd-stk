		 </xsd:annotation>
    <xsd:complexContent>
     <xsd:extension base="fuge:FuGE.Common.Measurement.MeasurementType">

   <xsd:sequence>


              <xsd:element name="_value" minOccurs="1" maxOccurs="1"> 
